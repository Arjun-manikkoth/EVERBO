<!DOCTYPE html>
<html lang="en">
<head>

	<title>EVERBO</title>
	
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="Colo Shop Template">
<link rel="stylesheet" type="text/css" href="styles/userStyles/bootstrap4/bootstrap.min.css">
<link href="plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="plugins/OwlCarousel2-2.2.1/owl.carousel.css">
<link rel="stylesheet" type="text/css" href="plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
<link rel="stylesheet" type="text/css" href="plugins/OwlCarousel2-2.2.1/animate.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/main_styles.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/responsive.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/footer.css">
<link rel="stylesheet" href="styles/userStyles/style.css">
<link rel="stylesheet" href="plugins/themify-icons/themify-icons.css">
<link rel="stylesheet" type="text/css" href="plugins/jquery-ui-1.12.1.custom/jquery-ui.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/single_styles.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/single_responsive.css">	
<link rel="stylesheet" type="text/css" href="styles/userStyles/categories_styles.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/categories_responsive.css">
<link rel="stylesheet" type="text/css" href="styles/userStyles/cart.css">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.10.7/dist/sweetalert2.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.min.js"></script>


</head>
<body>

	
<div class="super_container">

	<!-- Header -->

	<header class="header trans_300">

		<!-- Top Navigation -->

		<div class="top_nav">
			<div class="container">
				<div class="row">
					<div class="col-md-6">
						<div class="top_nav_left">Selling only the best fashion's online</div>
					</div>
					<div class="col-md-6 text-right">
						<div class="top_nav_right">
							<ul class="top_nav_menu">

								<!-- Currency / Language / My Account -->

								<li class="currency">
									<a href="#">
										INR
										<i class="fa fa-angle-down"></i>
									</a>
								</li>
								<li class="language">
									<a href="#">
										English
										<i class="fa fa-angle-down"></i>
									</a>
								</li>
							
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>

		<!-- Main Navigation -->
			
				<div class="main_nav_container">
					<div class="container">
						<div class="row">
							<div class="col-lg-12 text-right">
								<div class="logo_container">
									<a href="#">EVER<span>BO</span></a>
								</div>
								<nav class="navbar">
									<ul class="navbar_menu">
										<li><a href="/">home</a></li>
										<li><a href="/shop">shop</a></li>
										<li><a href="/#newarrivals">new arrivals</a></li>
										<li><a href="/#bestsellers">best sellers</a></li>
										<li><a href="#">contact us</a></li>
									</ul>
									<ul class="navbar_user">
										
										<%if(locals.loggedIn){%>
											<li>
												<a href="/entry"><i class="fa fa-heart" aria-hidden="true"></i>
												</a>
											</li>
										<li>
											<a href="/entry"><i class="fa fa-user" aria-hidden="true"></i>
											</a>
										</li>	
										<li class="checkout">
												<a href="/entry"><i class="fa fa-shopping-cart" aria-hidden="true"></i>
												</a>
										</li>
                    <%}else{%>
											<li>
												<a href="/wishlist"><i class="fa fa-heart" aria-hidden="true"></i>
													<%if(locals.session&& session.wishlistCount>0){%>
														<span  class="checkout_items"><%=session.wishlistCount%></span>
                            <%}%>
												</a>
											</li>
											<li>
												<a href="/profile"><i class="fa fa-user" aria-hidden="true"></i>
												</a>
											</li>
											<li class="checkout">
													<a href="/cart"><i class="fa fa-shopping-cart" aria-hidden="true"></i>
                            <%if(locals.session&& session.cartCount>0){%>
														<span  class="checkout_items"><%=session.cartCount%></span>
                            <%}%>
													</a>
												</li>
										<%}%>
									
									</ul>
									<div class="hamburger_container">
										<i class="fa fa-bars" aria-hidden="true"></i>
									</div>
								</nav>
							</div>
						</div>
					</div>
				</div>

	</header>
	<%if(locals.profile){%>
	<div class="containerbox">
		<div class="navbar-outer container">
			<div class="navbar-user">
				<a href="/profile"  class="items" data-route="/profile,/edit_profile"><div class="nav-options" ><i class="fa fa-user-circle mx-3" aria-hidden="true"></i>Profile</div></a>
				<a href="/address"  class="items" data-route="/address,/add_address,/edit_address"><div class="nav-options" ><i class="fa fa-address-card-o mx-3" aria-hidden="true"></i>My Address</div></a>
				<a href="/order"   class="items" data-route="/order"><div class="nav-options"><i class="fa fa-cart-plus mx-3" aria-hidden="true"></i>My Orders</div></a>
				<a href="/wallet"   class="items" data-route="/wallet"><div class="nav-options"><i class="fa fa-money mx-3" aria-hidden="true"></i>My Wallet</div></a>
				<a href="/coupon"   class="items" data-route="/coupon"><div class="nav-options"><i class="fa fa-tag mx-3" aria-hidden="true"></i>Coupons</div></a>
				<a href="/change_password" class="items" data-route="/change_password,/new_password,/confirm_password" ><div class="nav-options"  ><i class="fa fa-lock mx-3" aria-hidden="true"></i>Change Password</div></a>
				<div class="nav-options" onclick="confirmLogout()"><i class="fa fa-sign-out mx-3" aria-hidden="true"></i>Logout</div>
			</div>
			<%}%>